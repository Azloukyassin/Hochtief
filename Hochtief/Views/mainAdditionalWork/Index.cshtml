            <th>
                @Html.DisplayNameFor(model => model.additionalWorksPds05)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.additionalWorksPds06)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.additionalWorksActivity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.eventHours)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.additionalWorksTxt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.itemno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rowid)
