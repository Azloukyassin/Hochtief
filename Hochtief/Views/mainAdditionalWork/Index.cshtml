                @Html.DisplayFor(modelItem => item.additionalWorksPds04)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.additionalWorksPds05)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.additionalWorksPds06)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.additionalWorksActivity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.eventHours)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.additionalWorksTxt)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
                @Html.ActionLink("Details", "Details", new {  id=item.id  }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.id  })
            </td>
        </tr>
}
    </tbody>
</table>
