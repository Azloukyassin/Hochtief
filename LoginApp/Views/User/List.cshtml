@model IEnumerable<LoginApp.Models.UserTab>

@{
    ViewBag.Title = "List";
}

<h2>All Users </h2>

<p>
    @Html.ActionLink("Neue Benutzer hinzufügen", "AddOrEdit", "User")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Projektname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isAdmin)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Projektname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.password)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.isAdmin)
        </td>
       
    </tr>
}

</table>
