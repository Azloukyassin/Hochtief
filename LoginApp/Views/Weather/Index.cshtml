@model IEnumerable<LoginApp.Models.Weathertest>
@{
    ViewBag.Title = "Weather";
}
    <h2>Alle Einträge</h2>
<p>
    @Html.ActionLink("Neue Eintrag hinzufügen","AddOrEdit", "Weather")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.weather_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Temp_Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Temp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Temp_condition)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.weather_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Temp_Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Temp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Temp_condition)
        </td>
        
    </tr>
}
</table>
