@model IEnumerable<LoginApp.MDBSourceEquipment>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Neue Eintrag hinzufügen", "Create", "SourceEquipmentMBDAdmin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SourceEquipment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.En_Equipment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.De_Equipment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodeCompany)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SourceEquipment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.En_Equipment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.De_Equipment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodeCompany)
        </td>
     
        <td>
            @Html.ActionLink("Update", "Update", new {  id=item.SourceEquipment }) |
           
            @Html.ActionLink("Delete", "Delete", new {  id=item.SourceEquipment })
        </td>
    </tr>
}

</table>
